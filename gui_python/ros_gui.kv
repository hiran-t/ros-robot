<FloatLayout>:
    
    MDLabel:
        id: my_label
        text: "GUI YASKAWA ROBOT"
        halign: "center"
        size_hint: 1, 1
        pos_hint: {"center_x": .11, "center_y": .98}

  
    MDSlider:
        id: my_slider1
        min: -180
        max: 180
        value: 0
        hint: True
        size_hint: 0.3, 0.3
        pos_hint: {"center_x": 0.15, "center_y": 0.2}
        on_value_normalized: app.slider_function(self.value)

    MDLabel:       
        text: 'JOINT OF DEGREE LINK1: %s' % int(my_slider1.value)
        halign: "center"
        size_hint: 1, 1
        pos_hint: {"center_x": 0.15, "center_y": 0.15}

  
